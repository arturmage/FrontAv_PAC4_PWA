{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.csr.html",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
    "dataGroups": [
    {
      "name": "openlibrary-api",
      "urls": [
        "https://openlibrary.org/search.json**",
        "https://openlibrary.org/works/**.json"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "timeout": "5s",
        "maxSize": 100,
        "maxAge": "1d"
      }
    },
    {
      "name": "openlibrary-covers",
      "urls": ["https://covers.openlibrary.org/b/id/**"],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 200,
        "maxAge": "30d",
        "cacheOpaqueResponses": true
      }
    }
  ]
}
